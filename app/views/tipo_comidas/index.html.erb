<h1>Listing tipo_comidas</h1>

<table>
  <thead>
    <tr>
      <th>Tipocomida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_comidas.each do |tipo_comida| %>
      <tr>
        <td><%= tipo_comida.tipoComida %></td>
        <td><%= link_to 'Show', tipo_comida %></td>
        <td><%= link_to 'Edit', edit_tipo_comida_path(tipo_comida) %></td>
        <td><%= link_to 'Destroy', tipo_comida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo comida', new_tipo_comida_path %>
