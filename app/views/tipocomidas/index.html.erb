<h1>Listing tipocomidas</h1>

<table>
  <thead>
    <tr>
      <th>Restaurante</th>
      <th>Tipocomida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipocomidas.each do |tipocomida| %>
      <tr>
        <td><%= tipocomida.restaurante_id %></td>
        <td><%= tipocomida.tipocomida %></td>
        <td><%= link_to 'Show', tipocomida %></td>
        <td><%= link_to 'Edit', edit_tipocomida_path(tipocomida) %></td>
        <td><%= link_to 'Destroy', tipocomida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipocomida', new_tipocomida_path %>
